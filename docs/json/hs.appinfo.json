{
  "name": "hs.appinfo",
  "swift": {
    "protocols": [
      {
        "name": "HSAppInfoModuleAPI",
        "type": "protocol",
        "methods": [],
        "properties": [
          {
            "name": "appName",
            "signature": "var appName: String { get }",
            "documentation": "The application's internal name (e.g., \"Hammerspoon 2\")"
          },
          {
            "name": "displayName",
            "signature": "var displayName: String { get }",
            "documentation": "The application's display name shown to users"
          },
          {
            "name": "version",
            "signature": "var version: String { get }",
            "documentation": "The application's version string (e.g., \"2.0.0\")"
          },
          {
            "name": "build",
            "signature": "var build: String { get }",
            "documentation": "The application's build number"
          },
          {
            "name": "minimumOSVersion",
            "signature": "var minimumOSVersion: String { get }",
            "documentation": "The minimum macOS version required to run this application"
          },
          {
            "name": "copyrightNotice",
            "signature": "var copyrightNotice: String { get }",
