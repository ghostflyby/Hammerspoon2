{
  "source": {
    "include": ["docs/json/combined"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "docs/api",
    "recurse": true,
    "readme": "docs/README.md",
    "template": "node_modules/clean-jsdoc-theme"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  }
}
